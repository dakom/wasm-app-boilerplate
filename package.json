{
  "name": "my_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run _rust:build:dev && npm-run-all -p _webpack:start _rust:start",
    "clean": "rimraf ./dist && rimraf ./target && npm-run-all -p _rust:core:clean _rust:fractal:clean",
    "bundle": "npm run _bundle:ci",
    "bundle:local": "npm run _bundle:ci && npm run _copy:static",
    "test": "npm-run-all -s _typescript:test _rust:core:test _rust:fractal:test",
    "//CI": "",
    "_bundle:ci": "npm run clean && npm run _rust:release && npm run _webpack:release",
    "//TYPESCRIPT": "",
    "_typescript:test:watch": "npm run _rust:buildandbind:ts_test && watchexec -w ./crates/core/src -w ./typescript npm run _:test",
    "_typescript:test": "npm run _rust:buildandbind:ts_test && jest",
    "_typescript:jest": "jest",
    "//WEBPACK": "",
    "_webpack:release": "webpack --progress --color --mode production --config webpack.release.js",
    "_webpack:start": "webpack-dev-server --mode development --config webpack.dev.js",
    "//RUST-DEV": "",
    "_rust:start": "watchexec -w ./crates/core/src -w ./crates/fractal/src npm run _rust:buildandbind:dev",
    "_rust:buildandbind:dev": "npm run _rust:build:dev && npm run _rust:bindgen:dev",
    "_rust:buildandbind:ts_test": "npm run _rust:build:ts_test && npm run _rust:bindgen:ts_test",
    "_rust:build:dev": "cargo +nightly build --target wasm32-unknown-unknown",
    "_rust:build:ts_test": "cd crates/core && cargo +nightly build --features ts_test --no-default-features --target wasm32-unknown-unknown",
    "_rust:bindgen:dev": "npm-run-all -p _rust:core:bindgen:dev _rust:fractal:bindgen:dev",
    "_rust:bindgen:ts_test": "wasm-bindgen ./target/wasm32-unknown-unknown/debug/my_core.wasm --target nodejs --out-dir ./_static/wasm/core/pkg",
    "//RUST-RELEASE": "",
    "_rust:release": "npm run _rust:build:release && npm run _rust:bindgen:release && npm run _rust:opt",
    "_rust:build:release": "cargo +nightly build --release --target wasm32-unknown-unknown",
    "_rust:bindgen:release": "npm-run-all -p _rust:core:bindgen:release _rust:fractal:bindgen:release",
    "_rust:opt": "npm-run-all -p _rust:core:opt _rust:fractal:opt",
    "//RUST-CORE": "",
    "_rust:core:test": "cd crates/core && cargo +nightly test",
    "_rust:core:clean": "rimraf ./_static/wasm/core/pkg && rimraf ./crates/core/target && rimraf ./crates/core/pkg",
    "_rust:core:bindgen:dev": "wasm-bindgen ./target/wasm32-unknown-unknown/debug/my_core.wasm --target web --out-dir ./_static/wasm/core/pkg",
    "_rust:core:bindgen:release": "wasm-bindgen ./target/wasm32-unknown-unknown/release/my_core.wasm --target web --out-dir ./_static/wasm/core/pkg",
    "_rust:core:opt": "cross-env BINARYEN_CORES=1 wasm-opt -O3 -o ./_static/wasm/core/pkg/my_core_bg.wasm ./_static/wasm/core/pkg/my_core_bg.wasm",
    "//RUST-FRACTAL": "",
    "_rust:fractal:test": "cd crates/fractal && cargo +nightly test",
    "_rust:fractal:clean": "rimraf ./_static/wasm/fractal/pkg && rimraf ./crates/fractal/target && rimraf ./crates/fractal/pkg",
    "_rust:fractal:bindgen:dev": "wasm-bindgen ./target/wasm32-unknown-unknown/debug/my_fractal.wasm --no-typescript --target no-modules --no-modules-global wasm_fractal --out-dir ./_static/wasm/fractal/pkg",
    "_rust:fractal:bindgen:release": "wasm-bindgen ./target/wasm32-unknown-unknown/release/my_fractal.wasm --no-typescript --target no-modules --no-modules-global wasm_fractal --out-dir ./_static/wasm/fractal/pkg",
    "_rust:fractal:opt": "cross-env BINARYEN_CORES=1 wasm-opt -O3 -o ./_static/wasm/fractal/pkg/my_fractal_bg.wasm ./_static/wasm/fractal/pkg/my_fractal_bg.wasm",
    "//COPY": "",
    "_copy:static": "cpy * ../dist --cwd=_static && cpy **/* ../../dist/wasm --cwd=_static/wasm --parents && cpy **/* ../../dist/media --cwd=_static/media --parents"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dakom/my_app.git"
  },
  "author": "David Komer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dakom/my_app/issues"
  },
  "homepage": "https://github.com/dakom/my_app#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "cpy-cli": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.0",
    "eslint": "^6.7.2",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.0.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "lit-html": "^1.1.2",
    "xstate": "^4.7.2"
  }
}
